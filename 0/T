{


    inlet
    {
        type            freestream;
        freestreamValue           $internalField;
    }



    outlet
    {
         type            zeroGradient;
    }


    front
    {
        type            freestream;
        freestreamValue           $internalField;
    }


    top
    {
        type            freestream;
        freestreamValue           $internalField;
    }

    roket
    {
         type            zeroGradient;
    }

        symmetry1 
    { 
        type            symmetryPlane; 
    } 
    symmetry2
    { 
        type            symmetryPlane; 
    } 


   /* nozzle
